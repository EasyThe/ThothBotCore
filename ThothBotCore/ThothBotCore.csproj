    <Authors>EasyThe</Authors>
    <PackageLicenseUrl>https://raw.githubusercontent.com/EasyThe/ThothBotCore/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl />
    <RepositoryUrl>https://github.com/EasyThe/ThothBotCore</RepositoryUrl>
